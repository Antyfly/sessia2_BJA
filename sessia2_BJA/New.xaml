<Window x:Class="sessia2_BJA.New"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:sessia2_BJA"
        mc:Ignorable="d"
        Title="new" Height="510" Width="300" Style="{StaticResource Window}" WindowStartupLocation="CenterScreen">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="1*"/>
            <RowDefinition Height="10*"/>
            <RowDefinition Height="1*"/>
        </Grid.RowDefinitions>

        <Label Grid.Row="0"
               Content="Добавление клиента"
               VerticalContentAlignment="Center"
               HorizontalAlignment="Center"
               FontSize="20" Margin="47,0,46.6,0.2" Width="200"/>
        <Image Source="/Image/Приятный шелест.png" HorizontalAlignment="Left" Margin="0,0,0,0.2" Width="47"/>
    </Grid>
</Window>
